<%- | $interface,
      $frame     = undef,
      $netnum    = undef,
      $pre_up,
      $up,
      $post_up,
      $pre_down,
      $down,
      $post_down,
| -%>
iface <%= $interface %> ipx static
<% if $frame { -%>
  frame <%= $frame %>
<% } -%>
<% if $netnum { -%>
  netnum <%= $netnum %>
<% } -%>
<%= epp('network/iface_events.epp', {
  pre_up    => $pre_up,
  up        => $up,
  post_up   => $post_up,
  pre_down  => $pre_down,
  down      => $down,
  post_down => $post_down,
}) %>

