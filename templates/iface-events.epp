<% $pre-up.each | $cmd | { -%>
  pre-up $cmd
<% } -%>
<% $up.each | $cmd | { -%>
  up $cmd
<% } -%>
<% $post-up.each | $cmd | { -%>
  post-up $cmd
<% } -%>
<% $pre-down.each | $cmd | { -%>
  pre-down $cmd
<% } -%>
<% $down.each | $cmd | { -%>
  down $cmd
<% } -%>
<% $post-down.each | $cmd | { -%>
  post-down $cmd
<% } -%>
